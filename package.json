{
  "name": "rcasto.dev",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:server": "http-server --port 3000 ./public",
    "start:dev": "npm-run-all clean watch:view",
    "build": "npm-run-all clean build:view",
    "build:view": "ts-node scripts/buildView.tsx",
    "watch:view": "chokidar 'views/**/*.ejs' 'projects/*.ts' 'src/*.tsx' -c 'npm run build:view' --initial",
    "clean": "rimraf public/index.html",
    "new": "npx ts-node scripts/generateProjectTemplate.ts"
  },
  "engines": {
    "node": "18.x.x",
    "npm": "9.x.x"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "3.1.2",
    "@types/node": "18.16.0",
    "@types/react-dom": "18.0.11",
    "chokidar-cli": "3.0.0",
    "ejs": "3.1.8",
    "http-server": "14.1.1",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "4.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
