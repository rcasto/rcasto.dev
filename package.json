{
  "name": "rcasto.dev",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:server": "nodemon server.js",
    "start:dev": "npm-run-all clean watch:view",
    "start-docker": "docker-compose up --detach --build",
    "build": "npm-run-all clean build:view",
    "build:view": "ts-node scripts/buildView.tsx",
    "watch:view": "chokidar 'views/**/*.ejs' 'projects/*.ts' 'src/*.tsx' -c 'npm run build:view' --initial",
    "clean": "rimraf public/index.html",
    "new": "npx ts-node scripts/generateProjectTemplate.ts"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "3.1.0",
    "@types/node": "17.0.21",
    "@types/react-dom": "18.0.1",
    "chokidar-cli": "3.0.0",
    "ejs": "3.1.6",
    "http-server": "14.1.0",
    "nodemon": "2.0.16",
    "npm-run-all": "4.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "express": "4.18.1"
  }
}
