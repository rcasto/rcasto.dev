{
  "name": "rcasto.dev",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm-run-all clean --parallel start build:dev",
    "build": "npm-run-all clean build:prod",
    "build:dev": "webpack --config webpack.config.js --mode development --watch",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "clean": "rimraf dist",
    "deploy": "npm-run-all build deploy:gcloud",
    "deploy:gcloud": "gcloud config set project rcasto-dev && gcloud app deploy app.yaml"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.4",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "helmet": "4.1.0",
    "social-contact": "1.0.9"
  },
  "devDependencies": {
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "4.2.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  }
}
